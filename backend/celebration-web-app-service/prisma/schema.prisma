// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model adminModel {
  id            Int       @id @default(autoincrement())
  adminName     String
  company       String
  email         String    @unique
  emailPassword String
  isDeleted     Int
}

model companyModel {
  id            Int       @id @default(autoincrement())
  companyName   String
  hrMail        String
  hrMailPassword String
  aLiveToMail   String
  aLiveCcMail   String
  aLiveBccMail  String
  isDeleted     Int
}